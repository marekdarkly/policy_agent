# LaunchDarkly AI Config
# Enable LaunchDarkly AI Config for per-agent model management
LAUNCHDARKLY_ENABLED=false
LAUNCHDARKLY_SDK_KEY=your_launchdarkly_sdk_key_here

# LLM Configuration (Fallback when LaunchDarkly is disabled)
# Use either OpenAI or Anthropic (comment out the one you don't use)

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
LLM_PROVIDER=openai
LLM_MODEL=gpt-4-turbo-preview

# Anthropic Configuration
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# LLM_PROVIDER=anthropic
# LLM_MODEL=claude-3-5-sonnet-20241022

# Application Settings
LOG_LEVEL=INFO
ENABLE_TRACING=false
